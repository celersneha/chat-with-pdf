{
  "name": "server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "cross-env": "^10.0.0"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "dev": "bun --watch run src/index.ts",
    "dev:worker": "bun --watch run worker.ts",
    "db:push": "bunx drizzle-kit push --config=drizzle.config.ts",
    "prod": "bun --env-file=.env.production run src/index.ts",
    "prod:worker": "bun --env-file=.env.production run worker.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clerk/express": "^1.7.21",
    "@langchain/community": "^0.3.51",
    "@langchain/core": "^0.3.72",
    "@langchain/google-genai": "^0.2.17",
    "@langchain/langgraph": "^0.4.8",
    "@langchain/mistralai": "^0.2.1",
    "@langchain/qdrant": "^0.1.3",
    "@mistralai/mistralai": "^1.9.17",
    "@upstash/qstash": "^2.8.2",
    "bullmq": "^5.58.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7"
  }
}
